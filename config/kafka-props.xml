<?xml version="1.0" ?>
<!-- KafkaBackside core properties.
	These will eventually include host and port values
	and other properties as required by various apps
	-->

<properties>

	<!-- App Topics -->
	<!--  	These apply strictly to to apps embedded in
		KafkaBackside.
		Clients outside use the opposite values -->
	<parameter name="ChatProducerTopic" value="chatout" />
	<parameter name="ChatConsumerTopic" value="chatin" />

	<parameter name="ElizaProducerTopic" value="elizaout" />
	<parameter name="ElizaConsumerTopic" value="elizain" />
	
	<!-- /App Topics -->
	
	<parameter name="KAFKA_SERVER_URL" value="localhost" />

	<parameter name="KAFKA_SERVER_PORT" value="9092" />
	
	<!-- Plugin Connector names and classpaths -->
	<list name="PlugInConnectors">
	<!--  	<parameter name="ElasticSearch" value="org.topicquests.backside.kafka.connector.es.ElasticSearchConnector" /> --> 
	</list>
	

</properties>

